        <dt class = "col-sm-2">
            Phone
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Dean.Phone)
        </dd>
        <dt class = "col-sm-2">
            Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Dean.Email)
        </dd>
    </dl>
</div>
<div>
        <a asp-action="Edit" class="functional-button" asp-route-id="@Model?.Id">Edit</a> |
        <a asp-action="Index" class="functional-button">Back to List</a>
